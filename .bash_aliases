# apt-get aliases 
alias agt-i='apt-get install'
alias agt-upd='apt-get update'
alias agt-upg='apt-get upgrade'
alias agt-ar='apt-get autoremove'

# Git aliases
alias g!='git init'
alias g.='git add .'
alias gc='git commit'
alias gs='git stash'
alias gb='git blame'
alias gcm='git commit -m'
alias gp='git push'
alias gpu='git pull'
alias gf='git fetch'
alias gch='git checkout'
alias gm='git merge'
alias gst='git status'
alias gl='git log'
alias gcl='git clone'


